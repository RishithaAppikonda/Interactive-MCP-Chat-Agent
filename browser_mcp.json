{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "airbnb": {
      "command": "npx",
      "args": [
        "@openbnb/mcp-server-airbnb"
      ]
    },
    "duckduckgo-search": {
      "command": "npx",
      "args": [
        "-y",
        "duckduckgo-mcp-server"
      ],
      "rateLimit": {
        "maxRequests": 1,
        "perSeconds": 3
      }
    }
  }
}
